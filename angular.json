{
  "version": 1,
  "projects": {
    "ng-rive": {
      "projectType": "library",
      "root": "libs/ng-rive",
      "sourceRoot": "libs/ng-rive/src",
      "prefix": "ng-rive",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ng-rive/tsconfig.lib.json",
            "project": "libs/ng-rive/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-rive/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ng-rive/src/**/*.ts",
              "libs/ng-rive/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ng-rive"],
          "options": {
            "jestConfig": "libs/ng-rive/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "recorder": {
      "projectType": "application",
      "root": "apps/recorder",
      "sourceRoot": "apps/recorder/src",
      "prefix": "ng-rive",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/recorder",
            "index": "apps/recorder/src/index.html",
            "main": "apps/recorder/src/main.ts",
            "polyfills": "apps/recorder/src/polyfills.ts",
            "tsConfig": "apps/recorder/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/recorder/src/favicon.ico",
              "apps/recorder/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/recorder/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/recorder/src/environments/environment.ts",
                  "with": "apps/recorder/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "recorder:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "recorder:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "recorder:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/recorder/src/**/*.ts",
              "apps/recorder/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/recorder"],
          "options": {
            "jestConfig": "apps/recorder/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "player": {
      "projectType": "application",
      "root": "apps/player",
      "sourceRoot": "apps/player/src",
      "prefix": "ng-rive",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/player",
            "index": "apps/player/src/index.html",
            "main": "apps/player/src/main.ts",
            "polyfills": "apps/player/src/polyfills.ts",
            "tsConfig": "apps/player/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/player/src/favicon.ico", "apps/player/src/assets"],
            "styles": ["apps/player/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/player/src/environments/environment.ts",
                  "with": "apps/player/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "player:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "player:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "player:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/player/src/**/*.ts",
              "apps/player/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/player"],
          "options": {
            "jestConfig": "apps/player/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "recorder"
}
